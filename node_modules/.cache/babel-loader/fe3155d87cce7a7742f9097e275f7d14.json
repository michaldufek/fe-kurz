stroy');\n    }\n\n    this.renderChart(this.chartData, this.options);\n    this.$emit('chart:render');\n  }\n}\n\nexport var reactiveData = {\n  data: function data() {\n    return {\n      chartData: null,\n    };\n  },\n  watch: {\n    chartData: dataHandler,\n  },\n};\nexport var reactiveProp = {\n  props: {\n    chartData: {\n      type: Object,\n      required: true,\n      default: function _default() {},\n    },\n  },\n  watch: {\n    chartData: dataHandler,\n  },\n};\nexport default {\n  reactiveData,\n  reactiveProp,\n};\n"]}]}